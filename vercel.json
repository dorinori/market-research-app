{
    "version": 2,
    "builds": [
      {
        "src": "api/server.py",
        "use": "@vercel/python",
        "config": {
          "runtime": "python3.9"
        }
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "api/server.py"
      },
      {
        "src": "/(.*)",
        "dest": "client/dist/$1",
        "headers": {
          "Cache-Control": "s-maxage=86400"
        }
      }
    ]
  }