{
    "version": 2,
    "builds": [
      {
        "src": "api/server.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "api/server.py"
      },
      {
        "src": "/assets/(.*)",
        "dest": "/dist/assets/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/dist/$1"
      }
    ]
  }