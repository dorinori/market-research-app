{
    "version": 2,
    "builds": [
      {
        "src": "api/server.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "50mb",
          "runtime": "python3.9",
          "includeFiles": "api/data/*.json"
        }
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/server.py"
      }
    ]
  }